<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    tools:context=".fragment.SignUpFragment"

    >
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    <!-- Confirm Password star Textview -->
    <TextView
        android:id="@+id/textView_yildiz5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="@string/star"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword2"
        app:layout_constraintStart_toEndOf="@+id/textView4" />

    <!-- Confirm Password Textbox -->
    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="193dp"
        android:layout_height="30dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:inputType="textPassword"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="TouchTargetSizeCheck" />

    <!--Confirm Password Textview -->
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/confirm_Password"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword" />

    <!-- Country star Textview -->
    <TextView
        android:id="@+id/textView_yildiz4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="@string/star"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toTopOf="@+id/spinner_country"
        app:layout_constraintStart_toEndOf="@+id/country_textview3" />

    <!-- City star Textview -->
    <TextView
        android:id="@+id/textView_yildiz2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="@string/star"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toTopOf="@+id/spinner_city"
        app:layout_constraintStart_toEndOf="@+id/city_textview2" />

    <!-- Country Textview -->
    <TextView
        android:id="@+id/country_textview3"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="Select Country"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/gender_textview"
        app:layout_constraintTop_toBottomOf="@+id/spinner_gender"
        tools:ignore="MissingConstraints" />

    <!-- City Spinner -->
    <AutoCompleteTextView
        android:id="@+id/spinner_city"
        android:layout_width="187dp"
        android:layout_height="29dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_text"
        android:completionThreshold="1"
        android:contentDescription="@string/Login"
        android:fontFamily="@font/inter_medium"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/city_textview2"
        app:layout_constraintTop_toBottomOf="@+id/city_textview2"
        tools:ignore="TouchTargetSizeCheck" />

    <!-- Country Spinner -->
    <AutoCompleteTextView
        android:id="@+id/spinner_country"
        android:layout_width="187dp"
        android:layout_height="29dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_text"
        android:completionThreshold="1"
        android:contentDescription="@string/Login"
        android:fontFamily="@font/inter_medium"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/country_textview3"
        app:layout_constraintTop_toBottomOf="@+id/country_textview3"
        tools:ignore="TouchTargetSizeCheck" />

    <!-- City  Textview -->
    <TextView
        android:id="@+id/city_textview2"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="Select City"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/country_textview3"
        app:layout_constraintTop_toBottomOf="@+id/spinner_country"
        tools:ignore="MissingConstraints" />
    <!-- Gender Spinner -->
    <AutoCompleteTextView
        android:id="@+id/spinner_gender"
        android:layout_width="187dp"
        android:layout_height="29dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_text"
        android:contentDescription="@string/Login"
        android:fontFamily="@font/inter_medium"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/gender_textview"
        app:layout_constraintTop_toBottomOf="@+id/gender_textview"
        tools:ignore="TouchTargetSizeCheck" />

    <!-- Gender star Textview -->
    <TextView
        android:id="@+id/textView7"
        android:layout_width="6dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="@string/star"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toTopOf="@+id/spinner_gender"
        app:layout_constraintStart_toEndOf="@+id/gender_textview" />

    <!-- Email Adress Textbox -->
    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="187dp"
        android:layout_height="29dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:hint="@string/Enter_your_mail_adress"
        android:inputType="textEmailAddress"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/mail_textview"
        app:layout_constraintTop_toBottomOf="@+id/mail_textview" />

    <!-- Name Textview -->
    <TextView
        android:id="@+id/textView_mail"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/view_name"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <!-- Name Textbox -->
    <EditText
        android:id="@+id/name_textview"
        android:layout_width="187dp"
        android:layout_height="29dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:hint="@string/Enter_name"
        android:inputType="textEmailAddress"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView_mail"
        app:layout_constraintTop_toBottomOf="@+id/textView_mail" />

    <!-- Gender Textview -->
    <TextView
        android:id="@+id/gender_textview"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="Choose Gender"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/mail_textview"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress"
        tools:ignore="MissingConstraints" />

    <!-- Mail Textview -->

    <TextView
        android:id="@+id/mail_textview"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/Email_Address"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView_mail"
        app:layout_constraintTop_toBottomOf="@+id/name_textview"
        tools:ignore="MissingConstraints" />

    <!-- Name Star Textview -->
    <TextView
        android:id="@+id/textView_yildiz3"
        android:layout_width="6dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="@string/star"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toTopOf="@+id/name_textview"
        app:layout_constraintStart_toEndOf="@+id/textView_mail" />

    <!-- Mail Star Textview -->
    <TextView
        android:id="@+id/textView_yildiz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="@string/star"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress"
        app:layout_constraintStart_toEndOf="@+id/mail_textview" />

    <!-- Password Textbox -->
    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="187dp"
        android:layout_height="29dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_medium"
        android:inputType="textPassword"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="TouchTargetSizeCheck" />

    <!-- Password visible Textview -->
    <View
        android:id="@+id/visible_password"
        android:layout_width="11dp"
        android:layout_height="10dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/eye"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPassword" />

    <!-- Confirm Password visible Textview -->
    <View
        android:id="@+id/visible_password2"
        android:layout_width="11dp"
        android:layout_height="10dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/eye"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword2"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword2"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPassword2" />

    <!-- City point view -->
    <View
        android:id="@+id/spinner_city_point"
        android:layout_width="12dp"
        android:layout_height="5dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/ok"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_city"
        app:layout_constraintEnd_toEndOf="@+id/spinner_city"
        app:layout_constraintTop_toTopOf="@+id/spinner_city" />

    <!-- Country point view -->
    <View
        android:id="@+id/spinner_country_point"
        android:layout_width="12dp"
        android:layout_height="5dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/ok"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_country"
        app:layout_constraintEnd_toEndOf="@+id/spinner_country"
        app:layout_constraintTop_toTopOf="@+id/spinner_country" />

    <!-- Gender point view -->
    <View
        android:id="@+id/spinner_gender_point"
        android:layout_width="12dp"
        android:layout_height="5dp"
        android:layout_marginEnd="5dp"
        android:background="@drawable/ok"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_gender"
        app:layout_constraintEnd_toEndOf="@+id/spinner_gender"
        app:layout_constraintTop_toTopOf="@+id/spinner_gender" />

    <!-- Password star Textview -->
    <TextView
        android:id="@+id/textView3"
        android:layout_width="6dp"
        android:layout_height="19dp"
        android:layout_marginBottom="5dp"
        android:text="@string/star"
        android:textColor="@android:color/holo_red_dark"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword"
        app:layout_constraintStart_toEndOf="@+id/textView" />

    <!-- Password Textview -->
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/Password"
        android:textColor="@color/black"
        android:textSize="12dp"
        app:layout_constraintStart_toStartOf="@+id/city_textview2"
        app:layout_constraintTop_toBottomOf="@+id/spinner_city" />

    <!-- Continue with Google -->

    <Button
        android:id="@+id/button_google"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:background="@drawable/reactangle2"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/ic_google_logo"
        android:fontFamily="@font/inter_medium"
        android:text="Continue with Google"
        android:textColor="@color/black"
        android:textAllCaps="false"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,RtlHardcoded,TouchTargetSizeCheck" />

    <!-- Sign up Button -->
    <TextView
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/rectangle_6"
        android:elevation="50dp"
        android:fontFamily="@font/laila_medium"
        android:gravity="center"
        android:outlineSpotShadowColor="@color/black"
        android:text="@string/Sign_up_button"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="19dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2" />

    <!-- Already have an account? Sign in Textview -->

    <!-- Or Textview -->
    <TextView
        android:id="@+id/textView2"
        android:layout_width="23dp"
        android:layout_height="25dp"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/inter_medium"
        android:text="Or"
        android:textColor="@color/black"
        android:textSize="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_google"
        tools:ignore="HardcodedText" />

    <!-- Left Line -->
    <View
        android:id="@+id/line2"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginStart="-140dp"
        android:layout_marginTop="12dp"
        android:background="@android:color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_google" />

    <!-- Right Line -->
    <View
        android:id="@+id/line"
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="-140dp"
        android:background="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_google" />

    <!-- Assistant image -->
    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="167dp"
        android:layout_height="162dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:src="@drawable/assistant"
        app:layout_constraintEnd_toStartOf="@+id/name_textview"
        app:layout_constraintHorizontal_bias="0.714"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line2" />

    <!-- Person image -->
    <TextView
        android:id="@+id/don_t_have_"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="@font/inter_regular"
        android:text="Have an account?Login"
        android:textSize="11dp"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="100dp"
        android:layout_height="212dp"
        android:layout_marginTop="-20dp"
        android:layout_marginEnd="-30dp"
        android:src="@drawable/casual"
        app:layout_constraintEnd_toStartOf="@+id/country_textview3"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_gravity="bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/bottom_logo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@drawable/logo" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>