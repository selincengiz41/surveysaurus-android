<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.UserInfoFragment">

    <TextView
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="-130dp"
        android:background="@drawable/update"
        android:elevation="50dp"
        android:fontFamily="@font/inter_bold"
        android:gravity="center"
        android:outlineSpotShadowColor="@color/black"
        android:text="@string/Update_button"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView15" />

    <EditText
        android:id="@+id/editTextTextEmailAddress3"
        android:layout_width="193dp"
        android:layout_height="30dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_regular"
        android:hint="@string/Enter_your_mail_adress"
        android:inputType="textEmailAddress"
        android:paddingStart="5dp"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_textview3" />

    <AutoCompleteTextView
        android:id="@+id/spinner_country4"
        android:layout_width="193dp"
        android:layout_height="30dp"

        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/rounded_text"
        android:contentDescription="@string/Login"
        android:fontFamily="@font/inter_regular"
        android:paddingStart="5dp"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_gender3"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextPassword7"
        android:layout_width="193dp"
        android:layout_height="30dp"

        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_regular"
        android:inputType="textPassword"
        android:paddingStart="5dp"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword6"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextPassword6"
        android:layout_width="193dp"
        android:layout_height="30dp"

        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_regular"
        android:inputType="textPassword"
        android:paddingStart="5dp"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_city4"
        tools:ignore="TouchTargetSizeCheck" />

    <AutoCompleteTextView
        android:id="@+id/spinner_city4"
        android:layout_width="193dp"
        android:layout_height="30dp"

        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/rounded_text"
        android:contentDescription="@string/Login"
        android:fontFamily="@font/inter_regular"
        android:paddingStart="5dp"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner_country4"
        tools:ignore="TouchTargetSizeCheck" />

    <AutoCompleteTextView
        android:id="@+id/spinner_gender3"
        android:layout_width="193dp"
        android:layout_height="30dp"

        android:layout_marginTop="25dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/rounded_text"
        android:contentDescription="@string/Login"
        android:fontFamily="@font/inter_regular"
        android:paddingStart="5dp"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress3"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/name_textview3"
        android:layout_width="193dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="70dp"
        android:background="@drawable/rounded_text"
        android:ems="10"
        android:fontFamily="@font/inter_regular"
        android:hint="@string/Enter_name"
        android:inputType="textEmailAddress"
        android:paddingStart="5dp"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView_mail3"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/view_name_info"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/name_textview3"
        app:layout_constraintEnd_toStartOf="@+id/name_textview3"
        app:layout_constraintTop_toTopOf="@+id/name_textview3"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/spinner_gender_point2"
        android:layout_width="12dp"
        android:layout_height="5dp"
        android:layout_marginStart="160dp"
        android:background="@drawable/ok"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_gender3"
        app:layout_constraintEnd_toEndOf="@+id/spinner_gender3"
        app:layout_constraintStart_toStartOf="@+id/spinner_gender3"
        app:layout_constraintTop_toTopOf="@+id/spinner_gender3" />

    <View
        android:id="@+id/spinner_country_point2"
        android:layout_width="12dp"
        android:layout_height="5dp"
        android:layout_marginStart="160dp"
        android:background="@drawable/ok"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_country4"
        app:layout_constraintEnd_toEndOf="@+id/spinner_country4"
        app:layout_constraintStart_toStartOf="@+id/spinner_country4"
        app:layout_constraintTop_toTopOf="@+id/spinner_country4" />

    <View
        android:id="@+id/visible_password7"
        android:layout_width="11dp"
        android:layout_height="10dp"
        android:layout_marginStart="160dp"
        android:background="@drawable/eye"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword7"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword7"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword7"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPassword7" />

    <View
        android:id="@+id/spinner_city_point2"
        android:layout_width="12dp"
        android:layout_height="5dp"
        android:layout_marginStart="160dp"
        android:background="@drawable/ok"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_city4"
        app:layout_constraintEnd_toEndOf="@+id/spinner_city4"
        app:layout_constraintStart_toStartOf="@+id/spinner_city4"
        app:layout_constraintTop_toTopOf="@+id/spinner_city4" />

    <View
        android:id="@+id/visible_password8"
        android:layout_width="11dp"
        android:layout_height="10dp"
        android:layout_marginStart="160dp"
        android:background="@drawable/eye"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword6"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword6"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword6"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPassword6" />

    <TextView
        android:id="@+id/gender_textview3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/laila_medium"
        android:text="@string/view_gender_info"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_gender3"
        app:layout_constraintEnd_toStartOf="@+id/spinner_gender3"
        app:layout_constraintTop_toTopOf="@+id/spinner_gender3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="Old Password:"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword6"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPassword6"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPassword6" />

    <TextView
        android:id="@+id/city_textview3"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/view_city_info"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_city4"
        app:layout_constraintEnd_toStartOf="@+id/spinner_city4"
        app:layout_constraintTop_toTopOf="@+id/spinner_city4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/mail_textview3"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/Email_Adress_info"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextEmailAddress3"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextEmailAddress3"
        app:layout_constraintTop_toTopOf="@+id/editTextTextEmailAddress3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="New Password:"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword7"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextPassword7"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPassword7" />

    <TextView
        android:id="@+id/country_textview4"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/inter_medium"
        android:text="@string/view_country_info"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner_country4"
        app:layout_constraintEnd_toStartOf="@+id/spinner_country4"
        app:layout_constraintTop_toTopOf="@+id/spinner_country4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="195dp"
        android:layout_marginEnd="195dp"
        android:fontFamily="@font/laila_medium"
        android:text="@string/personal_info"
        android:textColor="#000000"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/info"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextTextPassword7" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_gravity="bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/bottom_logo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:src="@drawable/logo"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>